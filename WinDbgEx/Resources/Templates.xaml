<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:WinDbgEx.ViewModels"
                    xmlns:ui="clr-namespace:WinDbgEx.UICore;assembly=WinDbgEx.UICore"
                    xmlns:view="clr-namespace:WinDbgEx.Views"
                    >

    <DataTemplate DataType="{x:Type vm:CommandViewModel}">
        <view:CommandView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:ModulesViewModel}">
        <view:ModulesView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:RegistersViewModel}">
        <view:RegistersView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:ThreadsViewModel}">
        <view:ThreadsView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:EventLogViewModel}">
        <view:EventLogView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:RunExecutableViewModel}">
        <view:RunExecutableView Margin="12" />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:AttachToProcessViewModel}">
        <view:AttachToProcessView Margin="12" />
    </DataTemplate>

</ResourceDictionary>