<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:WinDbgEx.ViewModels"
                    xmlns:view="clr-namespace:WinDbgEx.Views"
                    >
    <DataTemplate DataType="{x:Type vm:CommandViewModel}">
        <view:CommandView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:ModulesViewModel}">
        <view:ModulesView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:RegistersViewModel}">
        <view:RegistersView />
    </DataTemplate>
</ResourceDictionary>